{
    "author": {
        "name": "Brandon Scott",
        "url": "https://github.com/brandongregoryscott"
    },
    "dependencies": {
        "lodash": "4.17.21"
    },
    "description": "ESLint plugin for making your code easier to read",
    "devDependencies": {
        "@types/common-tags": "1.8.1",
        "@types/jest": "27.5.1",
        "@types/lodash": "4.14.182",
        "@typescript-eslint/eslint-plugin": "5.27.1",
        "@typescript-eslint/parser": "5.27.1",
        "@typescript-eslint/utils": "5.27.1",
        "common-tags": "1.8.2",
        "esbuild": "0.14.43",
        "eslint": "8.16.0",
        "eslint-plugin-collation": "1.0.2",
        "eslint-plugin-typescript-sort-keys": "2.1.0",
        "eslint-remote-tester": "2.1.4",
        "eslint-remote-tester-repositories": "0.0.5",
        "jest": "28.1.0",
        "prettier": "2.6.2",
        "ts-jest": "28.0.3",
        "typescript": "4.7.2"
    },
    "engines": {
        "node": ">=14"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "lint",
        "code-style",
        "typescript",
        "eslint",
        "react",
        "react-hooks",
        "hooks",
        "eslint-plugin",
        "eslintplugin"
    ],
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "name": "eslint-plugin-collation",
    "peerDependencies": {
        "@typescript-eslint/parser": ">=5",
        "@typescript-eslint/utils": ">=5",
        "eslint": ">=6"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:brandongregoryscott/eslint-plugin-collation.git"
    },
    "scripts": {
        "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node --external:eslint --external:@typescript-eslint/utils",
        "build:dist": "tsc --noEmit && npm run build -- --minify",
        "build:watch": "npm run build -- --watch",
        "clean": "rm -rf dist",
        "deploy:docs": "cd documentation && yarn install && USE_SSH=true yarn deploy",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "lint:fix": "npm run lint -- --fix",
        "lint:remote": "eslint-remote-tester",
        "postpublish": "[[ $(git branch --show-current) == $git_main_branch ]] && npm run deploy:docs || echo \"Skipping deploy:docs\"",
        "prepublishOnly": "npm run clean && npm run build",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "version": "1.0.3"
}
